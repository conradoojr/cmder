ls          =   ls --show-control-chars -F --color $*
ll          =    ls --show-control-chars -F --color $* -la
pwd         =   cd
clear       =   cls
history     =   cat %CMDER_ROOT%\config\.history
unalias     =   alias /d $1
which       =   where $*

subl        =   "C:\Program Files\Sublime Text 3\sublime_text.exe" $*
aliases     =   "C:\Program Files\Sublime Text 3\sublime_text.exe" C:\Users\cjunior\Downloads\cmder\config
hosts       =   "C:\Program Files\Sublime Text 3\sublime_text.exe" $* C:\Windows\System32\drivers\etc\hosts

gb          =   git branch $*
gs          =   git status $*


